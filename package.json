{
    "name": "json-complete",
    "version": "1.0.0",
    "description": "An encoder that can turn any standard JavaScript data object or value into a form that can be serialized by JSON, and do the reverse with a decoder. This includes preserving referential integrity and deduplication of data in the encoded output.",
    "scripts": {
        "test": "./node_modules/gulp/bin/gulp.js test",
        "test-browser": "./node_modules/gulp/bin/gulp.js test-browser",
        "lint": "node ./node_modules/eslint/bin/eslint.js ./src",
        "build": "./node_modules/gulp/bin/gulp.js prod"
    },
    "author": "Dwight House",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "ssh://git@stash.cierecloud.com:7999/cier/json-complete.git"
    },
    "main": "./dist/main.cjs.min.js",
    "module": "./dist/main.esm.min.js",
    "esnext": "./dist/main.esm.min.js",
    "devDependencies": {
        "@babel/core": "7.1.6",
        "@babel/preset-env": "7.1.6",
        "babel-plugin-module-resolver": "3.1.1",
        "babel-plugin-transform-esm-to-cjs": "0.0.0",
        "babelify": "10.0.0",
        "browser-sync": "2.26.3",
        "browserify": "16.2.3",
        "del": "3.0.0",
        "eslint": "5.9.0",
        "gulp": "3.9.1",
        "gulp-babel": "8.0.0",
        "gulp-better-rollup": "3.4.0",
        "gulp-brotli": "1.2.1",
        "gulp-file": "0.4.0",
        "gulp-gzip": "1.4.2",
        "gulp-modify-file": "1.0.1",
        "gulp-rename": "1.4.0",
        "gulp-tape": "1.0.0",
        "gulp-terser": "1.1.5",
        "gulp-watch": "5.0.1",
        "gulp-zopfli": "1.0.0",
        "rollup-plugin-root-import": "0.2.3",
        "rollup-stream": "1.24.1",
        "run-sequence": "2.2.1",
        "tape": "4.9.1",
        "vinyl-buffer": "1.0.1",
        "vinyl-source-stream": "2.0.0"
